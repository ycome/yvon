{
  "name": "yvon",
  "productName": "yvon",
  "description": "app for ynov, yvon bot",
  "author": {
    "name": "Louis AMSTUTZ",
    "email": "louis.amstutz@gmail.com"
  },
  "version": "0.3.2",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng build && electron .",
    "installoff": "electron-builder --dir",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build && electron .",
    "electron-aot": "ng build --prod && electron .",
    "package:linux": "ng build && electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "package:windows": "ng build && electron-packager . --overwrite --platform=win32 --arch=x64 --out=out",
    "deb64": "electron-installer-debian --src out/yvon-linux-x64/ --dest out/installers/ --arch amd64",
    "exe": "electron-installer-windows --src out/app-win32-x64/ --dest out/installers/"
  },
  "build": {
    "appId": "com.ynov.yvon",
    "mac": {
      "category": "utilitary"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.2",
    "@angular/cli": "1.6.6",
    "@angular/common": "5.2.2",
    "@angular/compiler": "5.2.2",
    "@angular/compiler-cli": "5.2.2",
    "@angular/core": "5.2.2",
    "@angular/forms": "5.2.2",
    "@angular/http": "5.2.2",
    "@angular/language-service": "5.2.2",
    "@angular/platform-browser": "5.2.2",
    "@angular/platform-browser-dynamic": "5.2.2",
    "@angular/router": "5.2.2",
    "@types/jasmine": "2.8.3",
    "@types/jasminewd2": "2.0.2",
    "@types/node": "6.0.60",
    "api-ai-javascript": "2.0.0-beta.21",
    "codelyzer": "4.0.1",
    "core-js": "2.4.1",
    "electron": "1.7.11",
    "milligram": "1.3.0",
    "ngx-electron": "1.0.4",
    "node-sass": "4.7.2",
    "protractor": "5.1.2",
    "rxjs": "5.5.6",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "2.5.3",
    "zone.js": "0.8.19"
  },
  "devDependencies": {
    "electron-builder": "19.55.2",
    "electron-installer-debian": "^0.8.0",
    "electron-installer-windows": "^0.2.0",
    "electron-packager": "10.1.1",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2"
  }
}